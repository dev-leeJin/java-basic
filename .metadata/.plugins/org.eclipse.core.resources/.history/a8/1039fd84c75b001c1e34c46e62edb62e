package accessmodifier;

public class Warrior {
	// name, hp, mp, lv, atk, def, exp;
	// 선언
	public String name;
	public int hp;
	public int mp;
	public int lv;
	public int atk;
	public int def;
	public int exp;
	
	public Warrior(String n) {
		name = n;
		hp = 20;
		mp = 10;
		lv = 1;
		atk = 3;
		def = 0;
		exp = 0;
		System.out.println("아이디가 생성되었습니다.");
	}
	

}
